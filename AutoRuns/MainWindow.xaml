<mah:MetroWindow x:Class="AutoRuns.MainWindow"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:local="clr-namespace:AutoRuns"
                 xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
                 mc:Ignorable="d"
                 WindowState="Maximized"
                 Title="AutoRuns">
    <TabControl Name="tabs">
        <TabItem Header="Logons">
            <local:LogonsTab></local:LogonsTab>
        </TabItem>
        <TabItem Header="Services">
            <local:ServicesTab></local:ServicesTab>
        </TabItem>
        <TabItem Header="Drivers">
            <local:DriversTab></local:DriversTab>
        </TabItem>
        <TabItem Header="Tasks">
            <local:TasksTab></local:TasksTab>
        </TabItem>
        <TabItem Header="KnownDLLs">
            <local:KnownDLLsTab></local:KnownDLLsTab>
        </TabItem>
    </TabControl>
</mah:MetroWindow>